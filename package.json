{
  "name": "whomsearch",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "set PORT=3001 && node ./bin/www",
    "dev": "concurrently \"npm run start\" \"cd client && npm run start\""
  },
  "dependencies": {
    "concurrently": "^5.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "constantinople": "^3.1.1",
    "clean-css": "^4.1.11"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "prettier": "^2.0.5"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {}
  },
  "prettier": {
    "trailingComma": "all",
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed"
  }
}
